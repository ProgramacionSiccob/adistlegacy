{
  "plugins": ["jsdoc-http-plugin"],
  "recurseDepth": 10,
  "source": {
    "include": ["app", "src"],
    "includePattern": ".+\\.js(doc|x)?$",
    "excludePattern": "(^|\\/|\\\\)_"
  },
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": false
  },
  "opts": {
    "template": "node_modules/docdash",
    "recurse": true,
    "destination": "./docs",
    "readme": "./README.md"
  }
}
