<?xml version="1.0" encoding="UTF-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping"
                  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                  xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping
                          https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">

    <entity name="Siccob\identificationAccess\user\domain\User" table="v_usuarios">
        <embedded name="id" class="UserId" use-column-prefix="false" />
        <embedded name="nickname" class="UserNickname" use-column-prefix="false" />
        <embedded name="password" class="UserPassword" use-column-prefix="false" />
        <embedded name="name" class="UserName" use-column-prefix="false" />
        <embedded name="email" class="UserEmail" use-column-prefix="false" />
        <embedded name="corporateEmail" class="UserCorporateEmail" use-column-prefix="false" />
        <embedded name="profile" class="UserProfile" use-column-prefix="false" />
        <embedded name="lastname" class="UserLastname" use-column-prefix="false" />
        <embedded name="status" class="UserStatus" use-column-prefix="false" />
    </entity>

</doctrine-mapping>